{
  "name": "healthcare",
  "version": "0.0.1",
  "description": "healthcare system for my graduation work",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16.13.x",
    "npm": "8.3.0",
    "yarn": "1.22.17"
  },
  "postinstall": "npm run build",
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t healthcare .",
    "docker:run": "docker run -p 3000:3000 -d healthcare",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "start": "node dist/index.js",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Edson Cizeski <cizeskiedson@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^8.0.1",
    "@loopback/authentication-jwt": "^0.10.1",
    "@loopback/boot": "^4.0.0",
    "@loopback/core": "^3.0.0",
    "@loopback/repository": "^4.0.0",
    "@loopback/rest": "^11.0.0",
    "@loopback/rest-explorer": "^4.0.0",
    "@loopback/security": "^0.6.1",
    "@loopback/service-proxy": "^4.0.0",
    "@sendgrid/mail": "^7.6.2",
    "aws-sdk": "^2.1051.0",
    "aws-serverless-express": "^3.4.0",
    "dotenv-safe": "^8.2.0",
    "loopback-connector-mongodb": "^6.1.0",
    "loopback-connector-postgresql": "^5.4.0",
    "mongodb": "^4.2.2",
    "serverless": "^2.70.0",
    "serverless-offline": "^8.3.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^8.0.0",
    "@loopback/eslint-config": "^12.0.0",
    "@loopback/testlab": "^4.0.0",
    "@types/node": "^10.17.60",
    "eslint": "^7.32.0",
    "source-map-support": "^0.5.20",
    "typescript": "~4.5.2"
  }
}
